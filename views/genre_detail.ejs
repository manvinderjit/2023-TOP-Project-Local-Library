<%- include('./partials/header'); -%>
    <h1>Genre: <%= genre.name %></h1>    
    <div style='margin-left:20px;margin-top:20px'>
        <h4>Books</h4>
        <ul>
            <% for (const book of genre_books) { %>
                <li>
                    <a href= <%= book.url %> > <%= book.title %> </a>
                    <p><%= book.summary %></p>
                </li>
            <% }; %>        
        </ul>
    </div>
    
<%- include('./partials/footer'); -%>